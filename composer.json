{
    "name": "keepsuit/laravel-temporal",
    "description": "Laravel temporal.io",
    "keywords": [
        "keepsuit",
        "laravel",
        "laravel-temporal"
    ],
    "homepage": "https://github.com/keepsuit/laravel-temporal",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabio Capucci",
            "email": "f.capucci@keepsuit.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "composer/class-map-generator": "^1.1",
        "illuminate/contracts": "^11.0 || ^12.0",
        "spatie/laravel-package-tools": "^1.92",
        "spiral/roadrunner": "^2024.3.3 || ^2025.0",
        "spiral/roadrunner-cli": "^2.6",
        "symfony/process": "^7.1.3",
        "temporal/sdk": "~2.14.0",
        "thecodingmachine/safe": "^2.0 || ^3.0"
    },
    "require-dev": {
        "dereuromark/composer-prefer-lowest": "^0.1.10",
        "larastan/larastan": "^3.4",
        "laravel/pint": "^1.17",
        "mockery/mockery": "^1.6",
        "nesbot/carbon": "^2.63 || ^3.0",
        "nunomaduro/collision": "^8.0 || ^9.0",
        "orchestra/testbench": "^9.1 || ^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan": "^2.1",
        "rector/rector": "^2.0",
        "spatie/invade": "^2.0",
        "spatie/laravel-data": "^4.3",
        "spatie/laravel-ray": "^1.26",
        "thecodingmachine/phpstan-safe-rule": "^1.2"
    },
    "suggest": {
        "spatie/laravel-data": "Can be used for workflows payloads (recommended ^4.3)"
    },
    "autoload": {
        "psr-4": {
            "Keepsuit\\LaravelTemporal\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keepsuit\\LaravelTemporal\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/roadrunner-temporal-worker"
    ],
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "test": "pest",
        "test-coverage": "pest --coverage",
        "lint": [
            "pint",
            "rector process --dry-run",
            "phpstan analyse"
        ],
        "lint:fix": [
            "pint",
            "rector process",
            "phpstan analyse"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Keepsuit\\LaravelTemporal\\LaravelTemporalServiceProvider"
            ]
        },
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
